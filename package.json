    "sass": "^1.77.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-shopify": "^3.0.1"
  },
  "dependencies": {
